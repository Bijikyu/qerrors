{
  "name": "@bijikyu/qerrors",
  "version": "3.0.0",
  "description": "Intelligent error handling middleware with AI-powered analysis, queue-based processing, observability endpoints, configurable sanitization, and production-ready monitoring plus logging. Uses industry-standard libraries (opossum, dotenv, lodash, @langchain/openai) for reliability and ecosystem integration, while providing OpenAI-based error suggestions, queue management, retry safeguards, and extensible configuration for Node.js applications.",
  "main": "index.js",
  "types": "lib/types.d.ts",
  "files": [
    "index.js",
    "lib/",
    "config/"
  ],
  "keywords": [
    "error",
    "logger",
    "middleware",
    "debugging",
    "AI",
    "openai",
    "environment",
    "validation",
    "caching",
    "retry",
    "queue",
    "winston",
    "express",
    "nodejs",
    "opossum",
    "dotenv",
    "lodash",
    "circuit-breaker",
    "resilience",
    "monitoring",
    "metrics",
    "health-check",
    "sanitization",
    "rate-limiting",
    "security"
  ],
  "author": "Q",
  "license": "ISC",
  "dependencies": {
    "@langchain/openai": "^0.6.17",
    "@types/pino": "^7.0.4",
    "axios": "^1.9.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "denque": "^2.1.0",
    "dotenv": "^17.2.3",
    "escape-html": "^1.0.3",
    "express-rate-limit": "^8.2.1",
    "express-slow-down": "^3.0.1",
    "fast-safe-stringify": "^2.1.1",
    "fileflows": "^1.0.5",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "lodash": "^4.17.21",
    "lru-cache": "^10.4.3",
    "node-cache": "^5.1.2",
    "opossum": "^9.0.0",
    "p-limit": "^7.2.0",
    "pino": "^10.1.1",
    "quick-lru": "^7.3.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bijikyu/qerrors"
  },
  "bugs": {
    "url": "https://github.com/Bijikyu/qerrors/issues"
  },
  "homepage": "https://github.com/Bijikyu/qerrors#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "start:dev": "node index.js",
    "test": "node tests/run-all.cjs",
    "test:unit": "node tests/basic.test.cjs",
    "test:integration": "node tests/integration.test.js",
    "test:ts": "tsc --noEmit && npm run test",
    "clean": "rm -rf dist",
    "lint": "eslint \"lib/**/*.js\" \"scripts/**/*.js\" \"tests/**/*.test.{js,cjs}\" \"*.js\"",
    "lint:fix": "eslint \"lib/**/*.js\" \"scripts/**/*.js\" \"tests/**/*.test.{js,cjs}\" \"*.js\" --fix",
    "format": "prettier --write \"lib/**/*.js\" \"scripts/**/*.js\" \"tests/**/*.{js,cjs,mjs}\" \"*.js\"",
    "validate": "node scripts/validate-config.js && npm run test:ts",
    "validate:config": "node scripts/validate-config.js",
    "deploy": "node scripts/deploy.js",
    "deploy:full": "./scripts/deploy.sh",
    "deploy:clean": "./scripts/deploy.sh --clean",
    "monitoring:setup": "node scripts/setup-monitoring.js",
    "prepublishOnly": "npm run validate && npm run build",
    "demo": "node demo/demo-server.js",
    "unqommented": "node scripts/unqommented.js"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "madge": "^8.0.0",
    "ts-node": "^1.7.1",
    "typescript": "^5.9.2"
  }
}
